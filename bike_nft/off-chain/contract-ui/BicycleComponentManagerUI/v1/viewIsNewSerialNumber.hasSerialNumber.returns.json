{
  "elements": [
    {
      "type": "input",
      "label": "This serial number",
      "id": "registerSerialNumber",
      "readOnly": true
    },
    {
      "type": "address",
      "label": "is registered to the address:",
      "id": "ownerAddress",
      "readOnly": true
    },
    {
      "type": "input",
      "label": "Info about this address:",
      "id": "ownerInfo",
      "readOnly": true
    },
    {
      "type": "button",
      "label": "Update address info",
      "onClick": {
        "default": "viewUpdateAddressInfo"
      }
    },
    {
      "type": "button",
      "label": "Back",
      "onClick": {
        "default": "viewEntry"
      }
    },
    {
      "type": "nft",
      "label": "Registered NFT:",
      "params": {
        "chainId": "contractNetwork",
        "contractAddress": "nftContractAddress",
        "tokenId": "nftTokenId"
      }
    },
    {
      "type": "text",
      "label": "If you're the owner or an administrator you can update the NFT metadata:"
    },
    {
      "type": "button",
      "label": "Update data (name, description, image)",
      "onClick": {
        "default": "viewUpdateNFT"
      }
    },
    {
      "type": "qrcode",
      "label": "Share the link to this page:",
      "params": {
        "basePath": "basePath",
        "relPath": "../viewIsNewSerialNumber",
        "registerSerialNumber": "registerSerialNumber"
      }
    }
  ]
}
