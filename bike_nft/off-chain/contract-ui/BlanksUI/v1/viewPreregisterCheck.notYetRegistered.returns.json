{
  "elements": [
    {
      "type": "text",
      "label": "Serial number not yet registered."
    },
    {
      "type": "input",
      "label": "The corresponding NFT token ID will be:",
      "id": "nftTokenId",
      "value": "",
      "readOnly": true
    },
    {
      "type": "button",
      "label": "Back",
      "id": "dontRegisterNowGoBack",
      "onClick": {
        "default": "viewRegisterForm"
      }
    },
    {
      "type": "button",
      "label": "Register now!",
      "id": "registerButton",
      "onClick": {
        "default": "register",
        "success": "viewRegisterOnSuccess",
        "failure": "viewRegisterOnFailure"
      }
    }
  ]
}
